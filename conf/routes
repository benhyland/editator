# Websockets for intra-room events
GET		/editator			controllers.Application.editatorEvents

# Start page to load angular and app
GET		/begin				controllers.Application.editatorBegin

# Join room
POST	/join				controllers.Application.editatorJoin

# Map static resources from the /public folder to the /assets URL path
GET		/favicon.ico		controllers.Assets.at(path="/public", file="images/favicon.ico")
GET		/assets/*file		controllers.Assets.at(path="/public", file)
